<div class="container">
  <div class="row">
  <div class="col-md-5 offset-md-3">
    <h1>Create Space</h1>
  <%= simple_form_for(space) do |f| %>
  <div>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.submit "Create space", class:"btn btn-success"%>
    <%= f.input :photo, as: :file %>
  </div>
<% end %>
  </div>
  </div>
</div>
